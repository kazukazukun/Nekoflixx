<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Theme applied to the Android Window while the process is starting -->
    <style name="LaunchTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <!-- Use a simple solid color for the window background -->
        <item name="android:windowBackground">@android:color/black</item>
    </style>
    <!-- Theme applied to the Android Window as soon as the Flutter engine starts drawing -->
    <style name="NormalTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <!-- Optionally, use a transition drawable to smoothly transition from the native splash screen to Flutter -->
        <item name="android:windowBackground">@android:color/black</item>
    </style>
</resources>
